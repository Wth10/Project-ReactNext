{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true,
		"jQuery": true
	},
	"extends": [
		"airbnb-base",
		"next/core-web-vitals",
		"prettier",
		"eslint:recommended",
		"plugin:import/recommended",
		"plugin:import/errors",
		"import/warnings"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "off"
	}
}
